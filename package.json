{
  "name": "ai-agent-sdk-orchestrator",
  "version": "1.0.3",
  "description": "A comprehensive TypeScript SDK for building robust AI agent workflows with multi-model support, logging, and orchestration capabilities",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./cli": {
      "import": "./dist/cli.mjs",
      "require": "./dist/cli.js",
      "types": "./dist/cli.d.ts"
    }
  },
  "bin": {
    "ai-agent": "./dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "example:basic": "tsx --env-file=.env examples/basic-workflow.ts",
    "example:multi-model": "tsx --env-file=.env examples/multi-model-agent.ts",
    "example:stream": "tsx --env-file=.env examples/streaming-responses.ts",
    "example:tools": "tsx --env-file=.env examples/custom-tools.ts"
  },
  "keywords": [
    "ai",
    "agent",
    "orchestrator",
    "workflow",
    "llm",
    "openrouter",
    "vercel",
    "typescript",
    "sdk"
  ],
  "author": "Francis Mwaniki",
  "license": "MIT",
  "devDependencies": {
    "@types/inquirer": "^9.0.9",
    "@types/node": "^20.10.0",
    "@types/react": "19.1.12",
    "@types/react-dom": "19.1.9",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.54.0",
    "postcss": "8.5.6",
    "tailwindcss": "4.1.12",
    "tsup": "^8.0.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "@eslint/js": "9.34.0",
    "axios": "^1.6.0",
    "chalk": "^5.3.0",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "commander": "^11.1.0",
    "crypto": "1.0.1",
    "eventemitter3": "^5.0.1",
    "fs": "0.0.1-security",
    "inquirer": "^9.2.0",
    "lucide-react": "0.542.0",
    "next": "15.5.2",
    "ora": "^7.0.1",
    "p-queue": "^8.0.1",
    "p-retry": "^6.2.0",
    "path": "0.12.7",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "tailwind-merge": "3.3.1",
    "tailwindcss-animate": "1.0.7",
    "winston": "^3.11.0",
    "zod": "^3.22.0"
  },
  "peerDependencies": {
    "@ai-sdk/openai": "^0.0.0",
    "openai": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "@ai-sdk/openai": {
      "optional": true
    },
    "openai": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Franc-dev/ai-agent-sdk-orchestrator-v2.git"
  },
  "bugs": {
    "url": "https://github.com/Franc-dev/ai-agent-sdk-orchestrator-v2/issues"
  },
  "homepage": "https://github.com/Franc-dev/ai-agent-sdk-orchestrator-v2#readme"
}
